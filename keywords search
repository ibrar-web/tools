      // ============ LANGUAGE & TRENDING ============
      async function fetchTrendingKeywords(lang = "en") {
        try {
          const keywords = trendingByLanguage[lang] || trendingByLanguage["en"];
          const trendList = document.getElementById("trendList");
          trendList.innerHTML = "";
          keywords.forEach((keyword) => {
            const span = document.createElement("span");
            span.textContent = keyword;
            span.addEventListener("click", () => {
              document.getElementById("keyword").value = keyword;
              document.getElementById("searchBtn").click();
            });
            trendList.appendChild(span);
          });
        } catch (err) {
          console.error("Error fetching trends:", err);
        }
      }

      document.getElementById("language").addEventListener("change", (e) => {
        const lang = e.target.value;
        localStorage.setItem("ez_lang", lang);
        updateUITranslations(lang);
        fetchTrendingKeywords(lang);
      });

      document.getElementById("refreshTrends").addEventListener("click", () => {
        const lang = document.getElementById("language").value;
        fetchTrendingKeywords(lang);
      });

      // ============ TRANSLATION HELPER ============
      async function translateText(text, targetLang) {
        if (targetLang === "en") return text;
        try {
          const response = await fetch(
            `https://api.mymemory.translated.net/get?q=${encodeURIComponent(
              text
            )}&langpair=en|${targetLang}`
          );
          const data = await response.json();
          return data.responseData.translatedText || text;
        } catch (err) {
          return text;
        }
      }

      async function translateToEnglish(text, sourceLang) {
        if (sourceLang === "en") return text;
        try {
          const response = await fetch(
            `https://api.mymemory.translated.net/get?q=${encodeURIComponent(
              text
            )}&langpair=${sourceLang}|en`
          );
          const data = await response.json();
          return data.responseData.translatedText || text;
        } catch (err) {
          return text;
        }
      }

      // ============ SEARCH FUNCTIONALITY ============
      async function getSuggestions(query, lang = "en") {
        try {
          // Translate query to English if needed
          const englishQuery =
            lang !== "en" ? await translateToEnglish(query, lang) : query;

          const url = `https://api.datamuse.com/words?ml=${encodeURIComponent(
            englishQuery
          )}&max=25`;
          const res = await fetch(url);
          const data = await res.json();
          let suggestions = Array.isArray(data)
            ? data.map((item) => item.word)
            : [];

          // Translate results back to selected language if needed
          if (lang !== "en" && suggestions.length > 0) {
            suggestions = await Promise.all(
              suggestions.map((s) => translateText(s, lang))
            );
          }

          return suggestions;
        } catch (err) {
          console.error("Error:", err);
          return [];
        }
      }

      const searchBtn = document.getElementById("searchBtn");
      const keywordInput = document.getElementById("keyword");
      const resultsGrid = document.getElementById("results");
      const header = document.getElementById("resultsHeader");
      const loading = document.getElementById("loading");
      const noResults = document.getElementById("noResults");

      // ============ UPDATE UI TRANSLATIONS ============
      function updateUITranslations(lang) {
        const t = translations[lang] || translations["en"];
        document.getElementById("pageTitle").textContent = t.title;
        document.getElementById("pageSubtitle").textContent = t.subtitle;
        document.getElementById("searchBtn").textContent = t.search_btn;
        document.getElementById("keyword").placeholder = t.placeholder;
        document.getElementById("refreshTrends").textContent = t.refresh;
        document.getElementById("loading").textContent = t.loading;
        document.getElementById("noResults").textContent = t.no_results;
      }

      searchBtn.addEventListener("click", async () => {
        const keyword = keywordInput.value.trim();
        const lang = document.getElementById("language").value;
        if (!keyword) return alert("Please enter a keyword");

        resultsGrid.innerHTML = "";
        header.style.display = "none";
        noResults.style.display = "none";
        loading.style.display = "block";

        const suggestions = await getSuggestions(keyword, lang);
        loading.style.display = "none";

        if (!suggestions.length) {
          noResults.style.display = "block";
          return;
        }

        localStorage.setItem("ez_keyword", keyword);
        localStorage.setItem("ez_results", JSON.stringify(suggestions));

        resultsGrid.innerHTML = "";
        header.style.display = "flex";
        const t = translations[lang] || translations["en"];

        suggestions.forEach((s) => {
          const div = document.createElement("div");
          div.className = "result-item";
          div.textContent = s;
          div.addEventListener("click", () => {
            navigator.clipboard.writeText(s);
            div.textContent = "‚úÖ Copied!";
            setTimeout(() => {
              div.textContent = s;
            }, 1500);
          });
          resultsGrid.appendChild(div);
        });

        document.getElementById("copyBtn").onclick = () => {
          navigator.clipboard.writeText(suggestions.join("\n"));
          alert(t.copied);
        };

        document.getElementById("exportBtn").onclick = () => {
          const csv =
            "data:text/csv;charset=utf-8," +
            suggestions.map((k) => `"${k}"`).join("\n");
          const a = document.createElement("a");
          a.href = encodeURI(csv);
          a.download = `keywords_${keyword}.csv`;
          a.click();
        };

        document.getElementById("exportJson").onclick = () => {
          const json = JSON.stringify(
            { keywords: suggestions, exported: new Date().toISOString() },
            null,
            2
          );
          const blob = new Blob([json], { type: "application/json" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = `keywords_${keyword}.json`;
          a.click();
        };
      });

      // ============ AUTO-DETECT USER REGION & LANGUAGE ============
      async function detectUserRegion() {
        try {
          // Try to detect region (optional, won't break if fails)
          try {
            const response = await fetch("https://ipapi.co/json/", {
              timeout: 3000,
            });
            if (response.ok) {
              const data = await response.json();
              const country = data.country_name || "Unknown";
              document.getElementById(
                "userLocation"
              ).textContent = `üìç ${country}`;
            }
          } catch (e) {
            // Silently fail - region detection is optional
            document.getElementById("userLocation").textContent = "üìç Global";
          }

          // Auto-detect language based on browser
          const browserLang = navigator.language.split("-")[0];
          const supportedLangs = Object.keys(translations);
          const detectedLang = supportedLangs.includes(browserLang)
            ? browserLang
            : "en";

          const savedLang = localStorage.getItem("ez_lang") || detectedLang;
          document.getElementById("language").value = savedLang;
          fetchTrendingKeywords(savedLang);
        } catch (err) {
          console.error("Error detecting region:", err);
          const savedLang = localStorage.getItem("ez_lang") || "en";
          document.getElementById("language").value = savedLang;
          fetchTrendingKeywords(savedLang);
        }
      }

      // ============ INITIALIZATION ============
      window.addEventListener("load", () => {
        initTheme();
        detectUserRegion();

        const lang = document.getElementById("language").value;
        updateUITranslations(lang);

        const lastKeyword = localStorage.getItem("ez_keyword");
        if (lastKeyword) {
          keywordInput.value = lastKeyword;
        }
      });
    </script>
  </body>
</html>
